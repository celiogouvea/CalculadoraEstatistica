<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cgtechnology.com.calculadoraestatistica">

    <application
        android:allowBackup="false"

        android:icon="@drawable/icone"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TelaPrincipal" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        //Sobre
        <activity android:name=".TelaSobre"
            android:label="@string/app_name"/>

        //Tela consulta
        <activity android:name=".TelaRol"
            android:label="@string/app_name"/>

        //Tela cadastros
        <activity android:name=".TelaCadastro"
            android:label="@string/app_name"/>

        //Tela Tabela
        <activity android:name=".TelaTable"
            android:label="@string/app_name"/>

        //Tela Grapihc
        <activity android:name=".Graphic"
            android:label="@string/app_name"/>



        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>

</manifest>